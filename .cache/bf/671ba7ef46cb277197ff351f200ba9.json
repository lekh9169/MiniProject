:112,"column":20},"source":"components/CreatePost.jsx","original":{"line":51,"column":8}},{"generated":{"line":112,"column":21},"source":"components/CreatePost.jsx","original":{"line":32,"column":8}},{"generated":{"line":112,"column":22},"source":"components/CreatePost.jsx","original":{"line":31,"column":4}},{"generated":{"line":113,"column":0},"source":"components/CreatePost.jsx","original":{"line":68,"column":1}},{"generated":{"line":113,"column":1},"source":"components/CreatePost.jsx","original":{"line":7,"column":0}},{"name":"CreatePost","generated":{"line":115,"column":15},"source":"components/CreatePost.jsx","original":{"line":69,"column":15}}],"sources":{"components/CreatePost.jsx":"import React,{useState} from 'react'\r\nimport { PageHeader,Input,Button} from 'antd';\r\nconst {TextArea}=Input;\r\nimport db from '../firebase'\r\nimport { navigate } from \"@reach/router\"\r\n\r\nconst CreatePost=(props)=>{\r\n    const [title,setTitle]=useState('')\r\n    const [content,setContent]=useState('')\r\n    const onTitleChange=(event)=>{\r\n        setTitle(event.target.value)\r\n    }\r\n    const onContentChange=(event)=>{\r\n        setContent(event.target.value)\r\n    }\r\n    const onCreatePost=()=>{\r\n        let postRef=db.collection('users').doc(props.user.uid).collection('posts')\r\n        let postRef2=db.collection('posts')\r\n        let payload={title,content}\r\n        postRef.add(payload)\r\n            .then(function(doc){\r\n                console.log(\"Document Successfully written!\",doc.id)\r\n            })\r\n        postRef2.add(payload)\r\n            .then(function(doc){\r\n                console.log(\"Document Successfully written!\",doc.id)\r\n            })\r\n        navigate(`/posts`)\r\n\r\n    }\r\n    return(\r\n        <div className=\"create_post_container\">\r\n            <div className =\"page_header_container\">\r\n            <PageHeader\r\n                style={{\r\n                    border: '1px solid rgb(235, 237, 240)',\r\n                }}\r\n                title=\"Create Post\"\r\n            />\r\n        </div>\r\n        <div className=\"post_inputs_container\">\r\n            <div className=\"post_input_container\">\r\n                <div className=\"post_input_title\">\r\n                    <h2>Post Title</h2>\r\n                </div>\r\n                <div className = \"post_input\">\r\n                    <Input placeholder=\"Post title\"value={title} onChange={onTitleChange}/>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div className=\"post_inputs_container\">\r\n            <div className=\"post_input_container\">\r\n                <div className=\"post_input_title\">\r\n                    <h2>Post Content</h2>\r\n                </div>\r\n                <div className = \"post_input\">\r\n                    <TextArea rows={10} value ={content} onChange={onContentChange}/>\r\n                </div>\r\n            </div>\r\n            <div className=\"post_input_button\">\r\n                <Button type=\"primary\" size=\"large\" onClick={onCreatePost}>\r\n                    Create Post\r\n                </Button>\r\n            </div>\r\n        </div>\r\n        </div>\r\n    )\r\n}\r\nexport default CreatePost;"},"lineCount":null}},"error":null,"hash":"905a7a79873615fe9c15db719b24150c","cacheData":{"env":{}}}