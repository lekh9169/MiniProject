ate([])\r\n    useEffect(()=>{\r\n        let userId=props?.user.uid ? props?.user.uid : props.uid\r\n        db.collection('users').doc(userId).collection('posts')\r\n            .onSnapshot(async posts => {\r\n                let postsData= await posts.docs.map(post => {\r\n                    let data = post.data()\r\n                    let { id }= post\r\n    \r\n                    let payload={\r\n                        id,\r\n                        ...data\r\n                    } \r\n                    return payload\r\n                });\r\n                setPosts(postsData)\r\n            })\r\n    },[])\r\n    return(\r\n    <div className=\"posts_container\">\r\n        <div className =\"page_header_container\">\r\n            <PageHeader\r\n                style={{\r\n                    border: '1px solid rgb(235, 237, 240)',\r\n                }}\r\n                title=\"Posts\"\r\n            />,\r\n        </div>\r\n        <div className=\"articles_container\">\r\n            {\r\n                _.map(posts,(article,idx)=>(\r\n\r\n                    <PostSnippet\r\n                    key={idx}\r\n                    id={article.id}\r\n                     title={_.capitalize(article.title) } \r\n                    content = {article.content.substring(1,1000)}\r\n                    user={props.user}\r\n                    uid={props.uid}\r\n                    />\r\n                ))\r\n                        \r\n            }\r\n            \r\n        </div>\r\n        \r\n\r\n    </div>\r\n    )\r\n}\r\nexport default Posts;"},"lineCount":null}},"error":null,"hash":"3d743307c5574d74a4301d4bb5215bd7","cacheData":{"env":{}}}