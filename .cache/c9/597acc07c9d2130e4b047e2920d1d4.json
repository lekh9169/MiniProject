SignIn","generated":{"line":118,"column":15},"source":"components/SignIn.jsx","original":{"line":67,"column":15}}],"sources":{"components/SignIn.jsx":"import React,{useState} from 'react';\r\nimport { PageHeader ,Input,Button} from 'antd';\r\nimport {auth} from '../firebase'\r\nimport { navigate } from \"@reach/router\"\r\nconst SignIn = (props) => {\r\n    const [email,setEmail] = useState('')\r\n    const [password,setPassword] = useState('')\r\n    const onEmailChange = (event) => setEmail(event.target.value)\r\n    const onPasswordChange=(event)=>setPassword(event.target.value)\r\n    const onSignIn=()=>{\r\n        console.log(email,password)\r\n        auth.signInWithEmailAndPassword(email, password)\r\n        .then(function(result){\r\n            console.log('user signed in')\r\n        }).catch(function(error) {\r\n            // Handle Errors here.\r\n            var errorCode = error.code;\r\n            var errorMessage = error.message;\r\n            // ...\r\n          });\r\n          setEmail('')\r\n          setPassword('')\r\n          navigate(`/posts`)\r\n    }\r\n    return(\r\n        <div className = \"Sign_up_container\">\r\n             <div className =\"page_header_container\">\r\n            <PageHeader\r\n                style={{\r\n                    border: '1px solid rgb(235, 237, 240)',\r\n                }}\r\n                title=\"Sign In\"\r\n            />\r\n            </div>\r\n            <div className=\"sign_up_container_inputs\" style ={{marginTop : \"20px\" }}>\r\n            <div className=\"post_input_container\">\r\n                <div className=\"post_input_title\">\r\n                    <h2>Email</h2>\r\n                </div>\r\n                <div className = \"post_input\">\r\n                    <Input placeholder=\"Email\" onChange={onEmailChange}/>\r\n                </div>\r\n            </div>\r\n            <div className=\"post_input_container\">\r\n                <div className=\"post_input_title\">\r\n                    <h2>Password</h2>\r\n                </div>\r\n                <div className = \"post_input\">\r\n                    <Input.Password placeholder=\"Password\" onChange={onPasswordChange}/>\r\n                </div>\r\n            </div>\r\n            <div style ={{width:'100%'}}>\r\n                <div style={{float:'left'}}>\r\n                    <a href=\"sign_up\">Don't have an account,Sign Up</a>\r\n                </div>\r\n                <div className=\"post_input_button\">\r\n                <Button type=\"primary\" size=\"large\" onClick={onSignIn}>\r\n                    SignIn\r\n                </Button>\r\n                </div>\r\n            </div>\r\n            \r\n        </div>\r\n        </div>\r\n    )\r\n}\r\nexport default SignIn;"},"lineCount":null}},"error":null,"hash":"8a248dd7e567dd77f293de945f748e64","cacheData":{"env":{}}}